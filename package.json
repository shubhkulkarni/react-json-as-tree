{
  "name": "react-json-as-tree",
  "author": {
    "name": "Shubham Kulkarni <shubhamkulkarni.sh@gmail.com> (http://github.com/shubhkulkarni)",
    "email": "shubhamkulkarni.sh@gmail.com"
  },
  "description": "A highly customizable and a tiny pure React component to render JSON or Javascript object data as a tree.",
  "private": false,
  "version": "1.1.10",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/style.css",
  "license": "MIT",
  "homepage": "https://github.com/shubhkulkarni/react-json-as-tree",
  "bugs": {
    "url": "https://github.com/shubhkulkarni/react-json-as-tree/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shubhkulkarni/react-json-as-tree.git"
  },
  "keywords": [
    "react json tree",
    "json",
    "react",
    "react json viewer",
    "react json as tree",
    "javascript object tree",
    "React JSON tree",
    "JSON tree component",
    "React tree view",
    "JSON object tree",
    "JavaScript object tree",
    "React nested tree structure",
    "JSON tree renderer",
    "React hierarchical view",
    "JSON viewer React",
    "JSON to tree React",
    "React tree component",
    "JSON data tree React",
    "React tree structure",
    "React expandable tree",
    "JSON object explorer"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "add-styles": "node addStyles.script.js",
    "package-publish": "npm version patch && npm run build && npm run add-styles && npm publish"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "path": "^0.12.7",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^4.2.3"
  }
}
